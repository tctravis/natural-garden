---
import Container from "../components/Container.astro";

const { Element = 'section', settings = {} } = Astro.props;

const sectionId = {
    ...(settings.sectionId && { "id": `${settings.sectionId}` })
}
---

<Element {...sectionId} class={`odd:bg-content-section-odd even:bg-content-section-even py-8`}><Container><slot /></Container></Element>